{
  "name": "creatix-server",
  "version": "1.0.0",
  "description": "creatix backend",
  "main": "server.js",
  "scripts": {
    "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/apis/**/*.graphql",
    "types": "graphql-to-typescript ./src/schema.graphql ./src/typings/graph.d.ts",
    "build:types": "npm run pretypes && npm run types",
    "build": "rimraf ./build && tsc && copyfiles -u 1 src/apis/**/*.graphql ./build/apis",
    "postinstall": "npm run build",
    "start": "set NODE_ENV=production&& node build/server.js",
    "dev": "set NODE_ENV=development&&nodemon --exec ts-node src/server.ts",
    "test": "set NODE_ENV=test&&exit",
    "test:dev": "set NODE_ENV=test&&jest --watchAll -u",
    "test:e2e": "set NODE_ENV=test&&jest --config ./test/jest-e2e.json"
  },
  "engines": {
    "node": "10.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.6",
    "copyfiles": "^2.1.0",
    "faker": "^4.1.0",
    "jest": "^23.6.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.4.4",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.16.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@octokit/rest": "^16.16.0",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/etag": "^1.8.0",
    "@types/express": "^4.16.1",
    "@types/fb": "0.0.23",
    "@types/helmet": "0.0.43",
    "@types/image-size": "^0.7.0",
    "@types/joi": "^14.3.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/node": "^11.9.4",
    "@types/nodemailer": "^4.6.6",
    "@types/pg": "^7.4.13",
    "@types/qs": "^6.5.2",
    "@types/redis": "^2.8.12",
    "@types/sharp": "^0.22.2",
    "@types/shortid": "0.0.29",
    "axios": "^0.18.0",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "etag": "^1.8.1",
    "express": "^4.16.4",
    "fb": "^2.0.0",
    "googleapis": "^39.1.0",
    "gql-merge": "0.0.6",
    "graphql": "^14.2.1",
    "graphql-to-typescript": "^0.1.2",
    "graphql-tools": "^4.0.4",
    "graphql-yoga": "^1.17.4",
    "helmet": "^3.16.0",
    "image-size": "^0.7.4",
    "joi": "^14.3.1",
    "json-diff": "^0.5.4",
    "jsonwebtoken": "^8.4.0",
    "merge-graphql-schemas": "^1.5.8",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nodemailer": "^5.1.1",
    "nodemon": "^1.18.10",
    "pg": "^7.8.0",
    "qs": "^6.6.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.22.1",
    "shortid": "^2.2.14",
    "typeorm": "^0.2.13",
    "webpack": "^4.29.5"
  },
  "jest": {
    "verbose": false,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(tsx|ts)?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)x?$",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/",
      "<rootDir>/coverage/",
      "<rootDir>/src/typings"
    ]
  }
}
