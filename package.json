{
  "name": "creatix-server",
  "version": "1.0.0",
  "description": "creatix backend",
  "main": "server.js",
  "scripts": {
    "pretypes": "gql-merge --out-file ./src/schema.graphql ./src/apis/**/*.graphql",
    "types": "graphql-to-typescript ./src/schema.graphql ./src/typings/graph.d.ts",
    "build:types": "npm run pretypes && npm run types",
    "build": "rimraf ./build && tsc && copyfiles -u 1 src/apis/**/*.graphql build",
    "postinstall": "npm run build",
    "start": "set NODE_ENV=production&& node build/server.js",
    "dev": "set NODE_ENV=development&&nodemon --exec ts-node src/server.ts",
    "test": "exit"
  },
  "engines": {
    "node": "10.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
  },
  "dependencies": {
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/faker": "^4.1.5",
    "@types/fb": "0.0.23",
    "@types/helmet": "0.0.43",
    "@types/jest": "^24.0.6",
    "@types/joi": "^14.3.1",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.9.4",
    "@types/nodemailer": "^4.6.6",
    "@types/pg": "^7.4.13",
    "@types/qs": "^6.5.2",
    "@types/shortid": "0.0.29",
    "copyfiles": "^2.1.0",
    "faker": "^4.1.0",
    "graphql-to-typescript": "^0.1.2",
    "jest": "^23.6.0",
    "nodemon": "^1.18.10",
    "rimraf": "^2.6.3",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.4.4",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typeorm": "^0.2.13",
    "typescript": "^3.3.3",
    "@octokit/rest": "^16.16.0",
    "axios": "^0.18.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "fb": "^2.0.0",
    "googleapis": "^37.2.0",
    "gql-merge": "0.0.6",
    "graphql": "^14.2.1",
    "graphql-tools": "^4.0.4",
    "graphql-yoga": "^1.17.4",
    "helmet": "^3.16.0",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.4.0",
    "merge-graphql-schemas": "^1.5.8",
    "morgan": "^1.9.1",
    "nodemailer": "^5.1.1",
    "pg": "^7.8.0",
    "qs": "^6.6.0",
    "reflect-metadata": "^0.1.13",
    "shortid": "^2.2.14",
    "webpack": "^4.29.5"
  },
  "jest": {
    "verbose": false,
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "collectCoverageFrom": [
      "**/*.{ts,js}",
      "!**/node_modules/**",
      "!**/build/**",
      "!**/coverage/**",
      "!**/src/typings/**",
      "!**/src/entity/**",
      "!**/src/app.ts",
      "!**/src/server.ts",
      "!**/src/schema.ts",
      "!**/src/lib/connectdb.ts"
    ],
    "transform": {
      "^.+\\.(tsx|ts)?$": "ts-jest"
    },
    "collectCoverage": true,
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/webpack.config.js"
    ],
    "coverageReporters": [
      "text",
      "text-summary"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)x?$",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/",
      "<rootDir>/coverage/",
      "<rootDir>/src/typings"
    ]
  }
}
