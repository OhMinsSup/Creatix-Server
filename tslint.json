{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb", "tslint-config-prettier"
    ],
    "linterOptions": {
        "exclude": ["config/**/*.js", "node_modules/**/*."]
    },
    "jsRules": {},
    "rules": {
        "no-console": false,
        "import-name": false,
        "variable-name": false,
        "no-boolean-literal-compare": false,
        "object-shorthand-properties-first": false
    },
    "rulesDirectory": []
}